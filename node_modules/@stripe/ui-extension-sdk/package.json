{
  "name": "@stripe/ui-extension-sdk",
  "pay": {
    "type": "module"
  },
  "version": "8.8.1",
  "description": "The suite of functionality available to UI extensions in Stripe apps",
  "author": "Stripe, Inc.",
  "license": "MIT",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx"
    }
  },
  "scripts": {
    "bind": "uixtsm bind",
    "bind-all": "uixtsm bind-all",
    "build": "uixtsm build && type yalc &>/dev/null || exit 0 && cd ./dist && yalc push --changed",
    "clean": "rm -r ./dist",
    "convert-docs": "uixtsm convert-docs",
    "jest": "js-scripts jest",
    "lint": "js-scripts eslint --ext ts,tsx src",
    "release": "uixtsm release",
    "start": "npm-watch build",
    "test": "js-scripts jest --watchAll",
    "types": "js-scripts tsc",
    "version-update": "uixtsm version-update"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@remote-ui/core": "^2.1.10",
    "@remote-ui/react": "^4.5.6",
    "@remote-ui/rpc": "^1.3.0",
    "@remote-ui/testing": "^1.3.6",
    "@types/stylis": "^4.0.2",
    "invariant": "^2.2.4",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "@remote-ui/rpc": "^1.2.5",
    "stripe": ">= 8.195.0"
  },
  "devDependencies": {
    "@pay-js/scripts": "workspace:*",
    "@sail/engine": "^32.7.0",
    "@sail/icons": "workspace:*",
    "@sail/ui": "^32.7.0",
    "@stripe-internal/eslint-config-stripe": "workspace:*",
    "@stripe-internal/tailor-dashboard-components": "workspace:*",
    "@stripe-internal/ui-extension-sdk-manager": "workspace:*",
    "@swc/core": "1.2.121",
    "@swc/jest": "^0.2.20",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^28.0.0",
    "@types/react": "^17.0.2",
    "@types/react-reconciler": "^0.26.1",
    "chokidar": "^3.5.1",
    "flowgen": "^1.14.1",
    "fs-extra": "^10.0.0",
    "jest-junit": "^13.0.0",
    "jest-matcher-utils": "^28.0.0",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "stripe": "^9.11.0",
    "typescript": "^4.5.0-dev.20211206"
  },
  "keywords": [
    "stripe",
    "dashboard",
    "extensions",
    "sdk"
  ]
}